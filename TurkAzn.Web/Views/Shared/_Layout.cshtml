<body>

    <div id="app" class="theme-dark">

        <div class="view view-main">
            <div class="page">




                <div class="toolbar tabbar tabbar-labels toolbar-bottom">
                    <div class="toolbar-inner">

                        <a href="~/" data-animate="false" class="tab-link @(ViewContext.RouteData.Values["Controller"].ToString() == "Anasayfa" ? "tab-link-active" : "")">


                            <i class="icon f7-icons if-not-md">house_fill</i>
                            <i class="icon material-icons md-only">house</i>

                            <span class="tabbar-label">Anasayfa</span>
                        </a>


                        <a href="/anasayfa/kategori" data-animate="false" class="tab-link @(ViewContext.RouteData.Values["Action"].ToString() == "Kategori" ? "tab-link-active" : "")">
                            <i class="icon f7-icons if-not-md">doc_text_search</i>
                            <i class="icon material-icons md-only">search</i>
                            <span class="tabbar-label">Kategoriler</span>
                        </a>
                        <a href="/anasayfa/favori" data-animate="false" class="tab-link @(ViewContext.RouteData.Values["Action"].ToString() == "Favori" ? "tab-link-active" : "")">
                            <i class="icon f7-icons if-not-md">heart</i>
                            <i class="icon material-icons md-only">favorite</i>
                            <span class="tabbar-label">Favorilerim</span>
                        </a>
                    @if (!User.Identity.IsAuthenticated)
                    {
                        <a href="/anasayfa/sepetim" data-animate="false" class="tab-link @(ViewContext.RouteData.Values["Action"].ToString() == "Sepetim" ? "tab-link-active" : "")">
                            <i class="icon f7-icons if-not-md">bag</i>
                            <i class="icon material-icons md-only">shopping_bag</i>
                            <span class="tabbar-label">Sepetim</span>
                        </a>
                    }
                    else
                    {
                        <a href="/sepet/index" data-animate="false" class="tab-link @(ViewContext.RouteData.Values["Controller"].ToString() == "Sepet" ? "tab-link-active" : "")">
                            <i class="icon f7-icons if-not-md">bag @await Html.PartialAsync("_SepetUrunSayisi")</i>
                            <i class="icon material-icons md-only">shopping_bag @await Html.PartialAsync("_SepetUrunSayisi")</i>
                            <span class="tabbar-label">Sepetim</span>
                        </a>

                    }
                        @if (!User.Identity.IsAuthenticated)
                        {


                        <a href="/anasayfa/hesabim" data-animate="true" data-login-screen=".login-screen" class="tab-link @(ViewContext.RouteData.Values["Action"].ToString() == "Hesabim" ? "tab-link-active" : "") login-screen-open">
                            <i class="icon f7-icons if-not-md">person</i>
                            <i class="icon material-icons md-only">person</i>
                            <span class="tabbar-label">Hesap</span>
                        </a>
                        }
                        else
                        {
                        <a href="/hesap/panel" data-animate="false" class="tab-link @(ViewContext.RouteData.Values["Controller"].ToString() == "Hesap" ? "tab-link-active" : "")">
                            <i class="icon f7-icons if-not-md">person</i>
                            <i class="icon material-icons md-only">person</i>
                            <span class="tabbar-label">Hesap</span>
                        </a>
                        }
                    </div>
                </div>

                @RenderBody()


                @if (!User.Identity.IsAuthenticated)
                {
                    
                        @Html.Partial("_LoginPartial")
                    
                }

            </div>
        </div>
    </div>


